    <!-- Grid -->

    <widget id="bzotech_grid_widgetproducts" class="BzoTech\WidgetProducts\Block\Widget\WidgetProducts"
            is_email_compatible="true"
            placeholder_image="Magento_Catalog::images/product_widget_new.png">
        <label translate="true">BZOTech - WidgetProducts Grid</label>
        <description translate="true">List of WidgetProducts</description>
        <parameters>
            <parameter name="widget_style" xsi:type="select" visible="true">
                <label translate="true">Style</label>
                <options>
                    <option name="widget_style" value="grid"
                            selected="true">
                        <label translate="true">Grid</label>
                    </option>
                </options>
            </parameter>

            <parameter name="widget_grid_title" xsi:type="text" required="true" visible="true">
                <label translate="true">Title</label>
            </parameter>

            <parameter name="product_source" xsi:type="select" visible="true">
                <label translate="true">Type Filter</label>
                <options>
                    <option name="product_source_lastest_products" value="lastest_products"
                            selected="true">
                        <label translate="true">New Products</label>
                    </option>
                    <option name="product_source_best_sellers" value="best_sellers">
                        <label translate="true">Best Sellers</label>
                    </option>
                    <option name="product_source_special_products" value="special_products">
                        <label translate="true">Special Products</label>
                    </option>
                    <option name="product_source_featured_products" value="featured_products">
                        <label translate="true">Featured Products</label>
                    </option>
                    <option name="product_source_countdown_products" value="countdown_products">
                        <label translate="true">Countdown Products</label>
                    </option>
                </options>
            </parameter>

            <parameter name="date_to" xsi:type="text" visible="true">
                <label translate="true">Date to</label>
                <depends>
                    <parameter name="product_source" value="countdown_products" />
                </depends>
            </parameter>

            <parameter name="select_category" xsi:type="text" visible="true">
                <label translate="true">Category IDs</label>
                <value>2</value>
            </parameter>

            <parameter name="product_order_by" xsi:type="select" visible="true">
                <label translate="true">Product Order By</label>
                <options>
                    <option name="product_order_by_desc" value="DESC"
                            selected="true">
                        <label translate="true">DESC</label>
                    </option>
                    <option name="product_order_by_asc" value="ASC">
                        <label translate="true">ASC</label>
                    </option>
                </options>
            </parameter>

            <parameter name="countdown_timer" xsi:type="select" visible="true">
                <label translate="true">Countdown Timer</label>
                <options>
                    <option name="countdown_timer_yes" value="1"
                            selected="true">
                        <label translate="true">Yes</label>
                    </option>
                    <option name="countdown_timer_no" value="0">
                        <label translate="true">No</label>
                    </option>
                </options>
            </parameter>

            <parameter name="product_limitation" xsi:type="text" required="true" visible="true">
                <label translate="true">Products Limit</label>
                <value>5</value>
            </parameter>

            <parameter name="custom_template" xsi:type="text" visible="true">
                <label translate="true">Custom Template</label>
                <description translate="true">
                    <![CDATA[Ex: widget/grid-custom.phtml. Note: This field is recommended for developers only]]>
                </description>
            </parameter>
        </parameters>
    </widget>

